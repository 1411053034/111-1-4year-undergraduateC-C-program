#include <stdio.h>

int main(void)
{
	int integer1;
	int integer2;

	printf("Enter first integer\n");

	scanf_s("%d", & integer1);

	printf("Enter secpmd integer\n");
	scanf_s("%d", & integer2);

	int sum;
	sum = integer1 + integer2;
	printf("Sum is %d\n", sum);
}
