#include <stdio.h>


double power(int, int);

int main(void)
{
	int x;
	printf("Please enter x:\n");
	scanf_s("%d", &x);
        printf("Please enter y:\n");
	scanf_s("%d", &y);

	printf("The power of x is:\n%.0f", power (x,y));

	return 0;
}

double power(int x,  int y)
{
    int result=1;
    int i;
    for (i =1; i<=y; i++)
        result = result * x;
    return result;
}
