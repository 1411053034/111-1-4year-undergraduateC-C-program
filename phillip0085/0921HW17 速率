int main(void)
{
    int u, a, t, v, s;
    printf("Enter initial velocity : \n");
    scanf_s("%d", &u);

    printf("Enter acceleration of the object:\n");
    scanf_s("%d", &a);

    printf("Enter the time elpsed:\n");
    scanf_s("%d", &t);

    v = u + a * t;
    s = u * t +  a * t * t/2;

    printf("Final velosity is %d\n", v);
    printf("Distance transversed is %d\n", s);
}
