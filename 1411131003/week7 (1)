#include <stdio.h>

unsigned long long int f(unsigned int number);

int main(void)
{
	unsigned int n;
	printf("please enter n:");
	scanf_s("%d",&n);

	printf("ç¬¬né …:%d\n", f(n - 1));
}
    

unsigned long long int f(unsigned int number)
{
	if (number == 0 or number ==1) {
		return number;
	}
	else {
		return (f(number - 1) + f(number - 2));
	}
}
