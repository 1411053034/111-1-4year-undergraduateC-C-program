#include <stdio.h>
#include <ctype.h>
#include <string.h>
int main() {
	char s1[17] = "suhanhanhello";
	char s2[17] = "han";
	int cnt = 0;
	printf("原始字串：%s\n", s1);
	printf("找尋字串：%s\n", s2);
	int i;
	for (i = 0; i < 16; i++) {
		if (strncmp(s2, s1 + i, 3) == 0)
			cnt++;
	}
	printf("出現次數為：%d", cnt);
}
//題目:給定兩個字串s1與s2，找出s2在s1出現的次數並印出。(請使用strncmp、strstr函數)
