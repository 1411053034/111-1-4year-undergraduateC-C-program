{
      
   printf("Enter a number\n" );
   scanf("%d",&n1);
   printf( "the results: " );



int faibonacci(n1){

   if(0==n1||1==n1){
   	return n1;
   }
      
   else{
   	return fibonacci(n1-1)+fibonacci(n1-2);
   	}
   }
 }
   

